{
  "name": "react-navigation-collapsible",
  "version": "5.5.0",
  "description": "An extension of react-navigation for the collapsible header",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benevbright/react-navigation-collapsible.git"
  },
  "author": "Bright Lee <benevbright@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react-native",
    "react-navigation",
    "navigation",
    "collapsible",
    "collapsible header",
    "hide header",
    "animate header",
    "hide navigation"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "prettier": "prettier --write index.ts  ./src/**/* README.md",
    "flowgen": "sh flow.sh",
    "lint": "eslint --ext '.js,.ts,.tsx' index.ts ./src",
    "build": "tsc && yarn flowgen",
    "clean": "rm -rf lib",
    "build-publish": "yarn clean && yarn lint && yarn build && yarn publish"
  },
  "peerDependencies": {
    "@react-navigation/native": "*",
    "@react-navigation/stack": "*",
    "react": ">=16.8",
    "react-native": ">=0.59"
  },
  "dependencies": {
    "react-native-iphone-x-helper": "^1.2.1"
  },
  "devDependencies": {
    "@types/react": "16.9.34",
    "@types/react-native": "0.62.5",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "flowgen": "^1.10.0",
    "prettier": "2.0.5",
    "typescript": "3.8.3"
  }
}
